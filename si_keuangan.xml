                  PRIMARY KEY (`id`)
                ) ENGINE=InnoDB AUTO_INCREMENT=20 DEFAULT CHARSET=latin1;
            </pma:table>
            <pma:table name="pembayaran">
                CREATE TABLE `pembayaran` (
                  `id` int(11) NOT NULL AUTO_INCREMENT,
