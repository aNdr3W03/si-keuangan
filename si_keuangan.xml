                  PRIMARY KEY (`id`)
                ) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=latin1;
            </pma:table>
            <pma:table name="guru">
                CREATE TABLE `guru` (
                  `id` int(11) NOT NULL AUTO_INCREMENT,
                  `name` varchar(50) NOT NULL,
                  `sex` enum('Pria','Wanita') NOT NULL,
                  `nip` varchar(15) NOT NULL,
                  `bidang` varchar(40) NOT NULL,
